        <record id="editing_tree_blux_inherit" model="ir.ui.view">
            <field name="name">Stock</field>
            <field name="model">stock.move.line</field>
            <field name="priority">1000</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
            <field name="arch" type="xml">
                <field name="location_dest_id" position="after">
                    <field name="code" invisible="1"/>
                    <field name="is_pick" invisible="1"/>
                    <field name="brand" invisible="1"/>
                    <field name="category" invisible="1"/>
                    <field name="generated_serial" force_save="1"/>
                    <field name="production_date" invisible="1"/>
                    <field name="exp_date"/>
                    <field name="barcode1" invisible="1"/>
                    <field name="barcode2" invisible="1"/>
                    <field name="use_date" invisible="1"/>
                    <field name="removal_date" invisible="1"/>
                    <field name="alert_date" invisible="1"/>
                    <field name="product_id" force_save="1"/>



                </field>
                <field name="product_uom_id" position="after">
                     <button name="unlink"
                            string="ASD"
                            type="object"
                            class="btn-secondary"
                             attrs="{'invisible': ['|',('code', '=', 'outgoing'),('is_pick', '=',True)]}"
                            icon="fa-trash-o"
                    />
<!--                    attrs="{'invisible': ['|',('code', '=', 'outgoing'),'&amp;',('is_pick', '=',True),('generated_serial', '>',0)]}"-->

                </field>
                    <xpath expr="//tree" position="attributes">
                        <attribute name="delete">0</attribute>
                        <attribute name="create">0</attribute>
                    </xpath>

            </field>
        </record>
        <record id="inherit_backorder_blux_form_view" model="ir.ui.view">
            <field name="name">inherit_backorder_blux_form_view</field>
            <field name="model">stock.backorder.confirmation</field>
            <field name="inherit_id" ref="stock.view_backorder_confirmation"/>
            <field name="arch" type="xml">
                <xpath expr="//form/footer/button[@name='process']" position="replace">
                    <button name="process" string="Create Backorder" type="object" class="oe_highlight"
                            groups="stock.group_stock_manager"/>
                </xpath>
                <xpath expr="//form/footer/button[@name='process_cancel_backorder']" position="replace">
                    <button name="process_cancel_backorder" string="No Backorder" type="object" class="btn-primary"
                            groups="stock.group_stock_manager"/>

                </xpath>

            </field>
        </record>
        <record id="operations_transfer_bulx_inherit_inventory" model="ir.ui.view">
            <field name="name">operations_transfer_bulx_inherit_inventory</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='picking_type_id']" position="after">
                    <field name="code" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="after">
                    <field name="image_medium" widget="image" class="oe_avatar"
                    />
                    <!--                    attrs="{'invisible':[('code','!=','outgoing')]}"-->
                    <field name="image_medium_two" widget="image" class="oe_avatar"
                    />
                    <!--                     attrs="{'invisible':[('code','!=','incoming')]}"-->
                    <field name="code" invisible="1"/>
                </xpath>

            </field>
        </record>
