                        <field name="is_pick" invisible="1"/>
                        <field name="is_internal" invisible="1"/>
                        <field name="is_generated" invisible="1"/>
                        <field name="location_source"
                               attrs="{'invisible':['|',('code', '=', 'incoming'),'&amp;',('is_internal', '!=',True),('is_pick', '!=',True)]}"/>


