        <record id="stock_move_detailed_operation_blux_form_view" model="ir.ui.view">
            <field name="name">stock_move_detailed_operation_blux_form_view</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_stock_move_operations"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group" position="after">

                    <group>
                        <field name="production_date"
                               attrs="{'invisible':[('code', '!=', 'incoming')],'readonly':[('is_generated', '=',True)],'required':[('code', '=', 'incoming')]}"/>
                        <field name="code" invisible="1"/>
                        <field name="quantity_to_generate"
                               attrs="{'invisible':[('code', '!=', 'incoming')],'readonly':[('is_generated', '=',True)]}"/>
                        <field name="location_scan"
                               attrs="{'invisible':['|',('code', '=', 'incoming'),('code', '=', 'outgoing')]}"/>
                        <field name="dest_location_scan"
                               attrs="{'invisible':[('is_internal', '!=',True)]}" />
                        <field name="search_serial" attrs="{'invisible':[('code', '=', 'incoming')]}"/>
